{
    "name": "sort-people-by-age",
    "version": "1.0.0",
    "main": "dist/src/index.js",
    "files": [
        "/src"
    ],
    "scripts": {
        "clean": "rimraf dist && rimraf coverage && rimraf sort-people-by-age.zip",
        "check": "npm run lint:fix && npm run prettier:write &&  npm run coverage",
        "audit": "npm audit",
        "lint:fix": "eslint . --ext .ts --fix",
        "lint": "eslint . --ext .ts",
        "prettier:list": "prettier --list-different \"./{test,src}//*.ts\" || ECHO .",
        "prettier:write": "prettier --write  \"./{test,src}//*.ts\"",
        "prettier": "prettier --check  \"./{test,src}//*.ts\"",
        "build": "npm run build-app && copy package.json dist && cd dist && npm run install-prod && cd ../ && npm run zip",
        "build-app": "npm run clean && tsc -p tsconfig.app.json",
        "zip": "bestzip dist/sort-people-by-age.zip dist/",
        "install-prod": "npm i --production"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.138",
        "@typescript-eslint/eslint-plugin": "^7.10.0",
        "@typescript-eslint/parser": "^7.10.0",
        "bestzip": "^2.2.1",
        "eslint": "^8.57.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5",
        "prettier-eslint": "^16.3.0",
        "prettier-eslint-cli": "^8.0.1",
        "rimraf": "^5.0.7",
        "ts-node": "^10.9.2",
        "tslint": "^6.1.3",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "aws-sdk": "^2.1633.0",
        "inversify": "^6.0.2",
        "reflect-metadata": "^0.2.2"
    }
}
